# 应用配置
PORT=3000
NODE_ENV=development

# Solana 网络配置
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_COMMITMENT=confirmed

# 领水相关配置
FAUCET_URL=你的领水接口地址
FAUCET_MAX_REQUESTS_PER_10_SECONDS=10
FAUCET_REQUEST_INTERVAL_MS=1000

# 钱包配置
WALLET_STORAGE_PATH=./wallets
WALLET_COUNT=50

# 日志配置
LOG_LEVEL=info 